{
  "name": "turbo-commit",
  "version": "0.6.0",
  "description": "CLI tool for Turbo Commit convention",
  "main": "./lib/cli.js",
  "scripts": {
    "test": "nyc jasmine-node --verbose --captureExceptions ./",
    "test:watch": "npm test & onchange bin/*.js lib/*.js ./*.spec.js -- npm test",
    "coverage": "nyc report --reporter=text-lcov > coverage.json",
    "release": "standard-version"
  },
  "engines": {
    "node": "6.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/labs-js/turbo-commit.git"
  },
  "keywords": [
    "tc",
    "turbo-commit",
    "commit",
    "git",
    "convetion",
    "cli",
    "change-log"
  ],
  "author": "js-labs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/labs-js/turbo-commit/issues"
  },
  "homepage": "https://github.com/labs-js/turbo-commit#readme",
  "dependencies": {
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "inquirer": "^1.1.2",
    "promise": "^7.1.1"
  },
  "bin": {
    "git-turbo": "./lib/git/git-turbo.js",
    "turbo": "./lib/cli.js"
  },
  "devDependencies": {
    "eslint": "^3.4.0",
    "jasmine-node": "^1.14.5",
    "nyc": "^8.1.0",
    "onchange": "^3.0.0",
    "standard-version": "^2.4.0"
  }
}
